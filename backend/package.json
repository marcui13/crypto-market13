{
  "name": "backend",
  "version": "0.0.1",
  "description": "The backend of a crypto-market app",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "16 || 18 || 20"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "echo clean",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run rebuild",
    "test": "mocha dist/__tests__",
    "posttest": "npm run lint",
    "test:dev": "mocha dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t backend .",
    "docker:run": "docker run -p 3000:3000 -d backend",
    "migrate": "node ./dist/migrate",
    "openapi-spec": "node ./dist/openapi-spec",
    "prestart": "npm run rebuild",
    "start": "node -r source-map-support/register .",
    "prepare": "npm run build",
    "rebuild": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git"
  },
  "author": "Agust√≠n <amarquardt@movizen.com>",
  "license": "",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^6.1.2",
    "@loopback/core": "^5.1.2",
    "@loopback/repository": "^6.1.2",
    "@loopback/rest": "^13.1.2",
    "@loopback/rest-explorer": "^6.1.2",
    "@loopback/service-proxy": "^6.1.2",
    "axios": "^1.5.0",
    "loopback-connector-mysql": "^7.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/eslint-config": "^14.0.3",
    "@loopback/testlab": "^6.1.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^16.18.40",
    "eslint": "^8.47.0",
    "mocha": "^10.2.0",
    "source-map-support": "^0.5.21",
    "typescript": "~5.1.6"
  }
}
